# include <stdio.h>
void main()
{
    int num,n1,n2,n3,n4 , sum;
    printf("Enter the four digit number ");
    scanf("%d",&num);
     n1= num % 10;     
    num /= 10;            

    n2= num % 10;     
    num /= 10;            

     n3 = num % 10;      
    num /= 10;            

    n4= num % 10;
    sum = n1 + n2+ n3+ n4 ;
    
    printf("Sum of the digits: %d\n", sum);
   
    
}
