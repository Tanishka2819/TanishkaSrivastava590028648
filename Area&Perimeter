# include<stdio.h>
int main ()
{
    int a ; 
    int b; 
    int area ;
    int perimeter;
    printf("Enter length ");
    scanf("%d",&a);
    printf("Enter width ");
    scanf("%d" ,&b);
    area = a*b;
    perimeter = 2*(a+b);
    printf("Area of rectangle = %d " ,area);
    printf("Perimeter of rectangle = %d " ,perimeter);
    return 0 ;
}
