#include <stdio.h>

int main()
{
    int num1;
    int num2;
    
    printf("Enter first number");
    scanf("%d",& num1);
    printf("Enter second number");
    scanf("%d",& num2);
    printf("%d \n",num1 );
    printf("%d \n",num2 );
    
    int sum;
    sum = num1 + num2 ;
    printf("Sum is %d",sum);

    return 0;
}
