#include<stdio.h>
int main ()
{
    int m1[10][10];
    int m2[10][10];
    int m3[10][10];
    int r1,c1,r2,c2;
    printf("Enter size of rows in 1st matrix:");
    scanf("%d",&r1);
    printf("Enter size of column in 1st matrix:");
    scanf("%d",&c1);
    printf("Enter size of rows in 2nd matrix:");
    scanf("%d",&r2);
    printf("Enter size of column in 2nd matrix:");
    scanf("%d",&c2);
    if ( c1 !=r2)
    {
        printf("Multiplication not possible");
    }
    printf("Enter element of 1st matrix:\n");
    for (int i = 0 ; i < r1 ; i++ )
    {
        for (int j = 0 ; j <c1 ; j++)
        {
            scanf(" %d", &m1[i][j]);
        }
    }
    printf("Enter element of 2nd matrix:\n");
    for (int i = 0 ; i < r2 ; i++ )
    {
        for (int j = 0 ; j <c2 ; j++)
        {
            scanf(" %d", &m2[i][j]);
        }
    }
    for (int i = 0; i < r1; i++)
    {
        for (int j = 0; j < c2; j++)
        {
             m3[i][j] = 0;
        }
    }
           
     for (int i= 0 ; i <r1 ; i++)
     {
         for (int j=0 ; j <c2; j++)
         {
             for(int k = 0 ; k<c1;k++)
             {
                 m3[i][j]+= m1[i][k] * m2[k][j];
             }
         }
     }
     printf("The product of matrices is :\n");
     for(int i = 0 ; i < r1 ; i ++)
     {
         for (int j = 0 ; j<c2;j++)
         {
             printf("%d",m3[i][j]);
             printf("");
         }
         printf("\n");
     }
     return 0 ;
}
