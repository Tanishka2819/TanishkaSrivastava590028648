#include <stdio.h>

int main()
{ int size ;
printf("Enter size of array:");
scanf("%d",& size);
 int a[size];
 
 for (int i = 0 ; i < size ; i++)
 {
  printf("Enter array element");
  scanf("%d",&a[i]);
 }
 int max = a[0] ;
 int min = a[0];
 for(int i = 0 ; i< size ; i++)
 {
     if (a[i]>max)
     {
         max=a[i];
     }
     if (a[i]<min)
     {
         min = a[i];
         
     }
 }
 printf("The biggest number %d\n:",max);
 printf("The smallest number %d:",min);
 return 0 ;
}
